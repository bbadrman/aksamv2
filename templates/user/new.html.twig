{% extends 'base.html.twig' %}

{% block title %}Ajouter un utilisateur - Aksam
{% endblock %}

{% block body %}

	<section class="content-header ">
		<h1>
			Utilisateurs
			<small>Gestion</small>
		</h1>
		<ol class="breadcrumb">
			<li>
				<a href="{{ path('app_user_index') }}">
					<i class="fa fa-users"></i>
					Tous les utilisateurs</a>
			</li>
			<li class="active">Ajouter un utilisateur</li>
		</ol>
