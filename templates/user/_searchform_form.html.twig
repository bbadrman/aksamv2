{{ form_start(search_form) }}
<div class="row" style="margin-right: 10px;">
	<div class="col-md-1" style="padding-right: 2px;padding-left: 8px;">
		{{ form_row(search_form.q, {attr: {class: 'form-control', 'placeholder': 'Rechercher un Utilisateur...'}}) }}
	</div>

	<div class="col-md-1" style="padding-right: 2px;padding-left: 8px;">
		{{ form_row(search_form.status, {attr: {class: 'form-control'}}) }}
	</div>
	<div class="col-md-1" style="padding-right: 2px;padding-left: 8px;">
		<button class="btn btn-default" type="submit">
			<i class="fa fa-search" aria-hidden="true"></i>
		</button>
	</div>
</div>
{{ form_end(search_form) }}
