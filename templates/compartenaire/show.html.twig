{% extends 'base.html.twig' %}

{% block title %}Compartenaire
{% endblock %}

{% block body %}
	<h1>Compartenaire</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ compartenaire.id }}</td>
			</tr>
			<tr>
				<th>Compagnie</th>
				<td>{{ compartenaire.compagnie }}</td>
			</tr>
			<tr>
				<th>Partenaire</th>
				<td>{{ compartenaire.partenaire }}</td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_compartenaire_index') }}">back to list</a>
	{% if is_granted('ROLE_DEV') %}
		<a href="{{ path('app_compartenaire_edit', {'id': compartenaire.id}) }}">edit</a>

		{{ include('compartenaire/_delete_form.html.twig') }}
	{% endif %}
{% endblock %}
